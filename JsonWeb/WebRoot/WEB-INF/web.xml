<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <display-name></display-name>	
  <servlet>
  <servlet-name>logsin</servlet-name>
  <servlet-class>com.json.Longsin</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>logsin</servlet-name>
  <url-pattern>/logsin</url-pattern>
  </servlet-mapping>
  <servlet>
  <servlet-name>rege</servlet-name>
  <servlet-class>com.json.Restet</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>rege</servlet-name>
  <url-pattern>/rege</url-pattern>
  </servlet-mapping>
  <servlet>
  <servlet-name>ajax</servlet-name>
  <servlet-class>com.json.ajaxTest</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>ajax</servlet-name>
  <url-pattern>/ajaxss</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>logsin.jsp</welcome-file>
  </welcome-file-list>
  
</web-app>
